@page "/"
@inject Toolbelt.Blazor.I18nText.I18nText I18nText

<h1>@Localizer.Index_Header</h1>


@code
{


    //Localizator
    I18nText.Index Localizer = new I18nText.Index();

    protected override async Task OnInitializedAsync()
    {
        Localizer = await I18nText.GetTextTableAsync<I18nText.Index>(this);
    }
}